<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="controllers.login.LoginController" >
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <AnchorPane layoutX="227.0" layoutY="115.0">
                    <children>
                        <Button id="loginButton" layoutX="130.0" layoutY="140.0" mnemonicParsing="false" text="Login"
                                textAlignment="CENTER" onAction="#loginAction"/>
                        <TextField fx:id="usernameBox" promptText="Username"/>
                        <PasswordField fx:id="passwordBox" layoutY="64.0" promptText="Password"/>
                        <Button layoutY="140.0" mnemonicParsing="false" text="Register" onAction="#registerAction"/>
                    </children>
                </AnchorPane>
            </children>
        </AnchorPane>
    </children>
</VBox>
